<%= link_to 'Log out', logout_path, method: "delete" if logged_in? %>
<%= link_to 'New event', new_event_path %>
<%= link_to "home", root_path %>

<%= render @user %>

<ul class="events">
  <%= render @events if !@events.nil? %>
</ul>

<p>Attending</p>
<% @attending.each do |x| %>
  <li><%= (link_to x.description, x) %></li>
<% end %>